{
  "expo": {
    "owner": "marole_pte",
    "name": "Spin Recargas",
    "slug": "ache",
    "privacy": "unlisted",
    "version": "1.0.22",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/first_screen_splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marole.ache",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.marole.ache",
      "versionCode": 1,
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_icon_foreground.png",
        "backgroundImage": "./assets/images/app_icon_background.png"
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "description": "",
    "extra": {
      "eas": {
        "projectId": "52b4b6ce-aa56-494c-bba7-3e8d9d98445c"
      }
    },
    "plugins": [
      "expo-asset",
      "expo-font",
      "expo-secure-store"
    ]
    
  }
}
